            <n:div sclass="col p-1">
                <n:span class="z-label font-weight-bold">Ecommerce Dashboard</n:span>
            </n:div>
        </bs-row>
        <bs-row>
            <apply templateURI="/pages/component/stateBoard.zul"/>
        </bs-row>
