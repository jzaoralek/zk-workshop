            <n:div sclass="col-md-5 p-1">
                ecommerce/sales.zul
            </n:div>
        </bs-row>
        <bs-row>
            <n:div sclass="col-md-5 p-1">
                ecommerce/marketShare.zul
            </n:div>
            <n:div sclass="col-md-7 p-1">
                ecommerce/import.zul
            </n:div>
        </bs-row>
        <bs-row>
            <n:div class="col-md-12 p-1">
                ecommerce/productTable.zul
            </n:div>
        </bs-row>
